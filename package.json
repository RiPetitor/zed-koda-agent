{
  "name": "koda-agent",
  "version": "0.4.1",
  "description": "KODA Agent Server for Zed Editor - AI coding assistant with permission control and planning modes",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./agent": "./src/agent/index.js",
    "./auth": "./src/auth/index.js",
    "./bridge": "./src/bridge/index.js",
    "./commands": "./src/commands/index.js",
    "./config": "./src/config/constants.js",
    "./models": "./src/models/index.js",
    "./session": "./src/session/index.js",
    "./tools": "./src/tools/index.js",
    "./utils": "./src/utils/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "agent": "node src/index.js",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:coverage": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "package": "node scripts/package-agent.mjs",
    "clean:zed": "bash scripts/clean-zed-cache.sh"
  },
  "keywords": [
    "koda",
    "zed",
    "agent",
    "acp",
    "ai",
    "coding-assistant"
  ],
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.12.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.0.0"
  }
}
